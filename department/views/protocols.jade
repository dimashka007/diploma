extends layout

block content
  div
    iframe#ifr(src="https://docs.google.com/document/d/15G16T5QGEXwr2cpMQ6Mv8nfIK0HoCwdRkRZLmRgZBxk/edit?usp=sharing" width="60%" height="100%" align="right" frameborder = "no")
    script.
      window.onload=function() {
      document.getElementById("ifr").style.height=window.innerHeight-77+"px";
      }
  .prot-list  
    ul.list-group
      each val in ProtList
        li.list-group-item
          a(href='/protocols/' + val)= val
